                                                                    <fragment>
                                                                        <string key="content">8. Разрешение споров
</string>
                                                                        <attributes>
                                                                            <font key="NSFont" size="14" name="TimesNewRomanPS-BoldMT"/>
                                                                            <paragraphStyle key="NSParagraphStyle" alignment="center" lineBreakMode="wordWrapping" baseWritingDirection="leftToRight" paragraphSpacing="5" paragraphSpacingBefore="5" tailIndent="467.74999999999994" defaultTabInterval="35.399999999999999" tighteningFactorForTruncation="0.0">
                                                                                <tabStops/>
